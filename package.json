{
    "name": "triple-digital-gmb",
    "version": "0.1.0",
    "dependencies": {
        "@google-cloud/bigquery": "^6.0.3",
        "@google-cloud/functions-framework": "^3.1.2",
        "@google-cloud/secret-manager": "^4.1.3",
        "axios": "^1.1.3",
        "axios-request-throttle": "^1.0.0",
        "dayjs": "^1.11.5",
        "express": "^4.18.2",
        "joi": "^17.9.2",
        "ndjson": "^2.0.0"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/express": "^4.17.14",
        "@types/jest": "^29.1.2",
        "@types/ndjson": "^2.0.1",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.25.0",
        "jest": "^29.2.0",
        "ts-jest": "^29.0.3",
        "typescript": "^4.8.4"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testRegex": "./src/.*.test.ts$",
        "setupFiles": [
            "dotenv/config"
        ],
        "testTimeout": 540000
    },
    "eslintConfig": {
        "env": {
            "es2021": true,
            "node": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/no-unused-vars": 0,
            "@typescript-eslint/no-explicit-any": 0
        }
    }
}
